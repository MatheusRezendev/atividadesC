/*

#include <stdio.h>

int fibonacci(int n) {
    if (n <= 1)
        return n;
    else
        return fibonacci(n - 1) + fibonacci(n - 2);
}

int main() {
    int posicao;

    printf("Digite a posição desejada na sequência de Fibonacci: ");
    scanf("%d", &posicao);

    if (posicao < 0) {
        printf("A posição deve ser um número não negativo.\n");
    } else {
        int resultado = fibonacci(posicao);
        printf("O número na posição %d da sequência de Fibonacci é: %d\n", posicao, resultado);
    }

    return 0;
}




